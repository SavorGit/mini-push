/* pages/template/commons.wxss */

/* ================================= 微信授权登录 ================================= */
.wxAuthLogin-container {
  width: 100%;
  height: 100%;
  text-align: center;
  border-radius: 20rpx;
}

.wxAuthLogin-container>.wxAuthLogin-top {
  padding: 50rpx 0rpx 10rpx 0rpx;
}

.wxAuthLogin-container>.wxAuthLogin-top>.opt {
  margin-top: -20rpx;
}

.wxAuthLogin-container>.wxAuthLogin-top>.opt>.close,
.wxAuthLogin-container>.wxAuthLogin-top>.opt>.close::before,
.wxAuthLogin-container>.wxAuthLogin-top>.opt>.close::after {
  position: fixed;
  right: 60rpx;
  margin: 0;
  padding: 0;
  border: 0;
  width: 30rpx;
  height: 30rpx;
  background-color: unset;
  line-height: 0;
}

.wxAuthLogin-container>.wxAuthLogin-top>.opt>.close>image {
  margin-right: 10rpx;
  width: 100%;
  height: 100%;
}

.wxAuthLogin-container>.wxAuthLogin-top>.title>label {
  font-weight: bold;
}

.wxAuthLogin-container>.wxAuthLogin-middle {
  padding: 20rpx 40rpx 20rpx 40rpx;
  margin-bottom: 20rpx;
}

.wxAuthLogin-container>.wxAuthLogin-middle>.content {
  display: block;
  margin-bottom: 20rpx;
}

.wxAuthLogin-container>.wxAuthLogin-middle>.link_relief {
  height: 60rpx;
  line-height: 60rpx;
}

.wxAuthLogin-container>.wxAuthLogin-middle>.link_relief>navigator {
  font-size: 32rpx;
  text-decoration: underline;
}

.wxAuthLogin-container>.wxAuthLogin-bottom>button {
  width: 50%;
  border-radius: 40rpx;
}

/* ================================= 固定悬浮按钮组 ================================= */
.fixedSuspensionButtonGroup {
  position: fixed;
  right: 0;
  bottom: 300rpx;
  width: 110rpx;
  height: 100rpx;
  z-index: 99;
}

.fixedSuspensionButtonGroup>.scan_tdc {
  width: 100%;
  height: 100%;
}

.fixedSuspensionButtonGroup>.scan_tdc>image {
  width: 100%;
  height: 100%;
}

/* ================================= 遥控器窗口 ================================= */
.remoteControlWindow-container {
  width: 100%;
  height: 100%;
  text-align: center;
  border-radius: 20rpx;
}

.remoteControlWindow-container .remoteControlWindow-control {
  padding: 20rpx;
  text-align: right;
}

.remoteControlWindow-container .remoteControlWindow-control image {
  width: 30rpx;
  height: 30rpx;
}

.remoteControlWindow-container .remoteControlWindow-top {
  padding: 10rpx 50rpx;
}

.remoteControlWindow-container .remoteControlWindow-top .opt button {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  border-radius: 60rpx;
  background-color: unset;
}

.remoteControlWindow-container .remoteControlWindow-top .opt>button icon {
  width: 100rpx;
  height: 100rpx;
}

.remoteControlWindow-container .remoteControlWindow-top .opt>button icon image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 49rpx;
}

.remoteControlWindow-container .remoteControlWindow-top .opt>button icon .fa-stack-1x {
  line-height: 2.4;
}

.remoteControlWindow-container .remoteControlWindow-middle {
  height: 580rpx;
  padding-top: 90rpx;
  z-index: 1;
}

.remoteControlWindow-container .remoteControlWindow-middle button {
  width: 400rpx;
  height: 400rpx;
  background: unset;
  border-radius: 200rpx;
  z-index: 1;
}

.remoteControlWindow-container .remoteControlWindow-middle button image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.remoteControlWindow-container .remoteControlWindow-middle .btn-tip {
  margin-top: -10rpx;
  height: 100rpx;
  font-size: 28rpx;
  z-index: 10;
}

.remoteControlWindow-container .remoteControlWindow-middle .btn-tip image {
  width: 50rpx;
  height: 50rpx;
  z-index: 10;
}

.remoteControlWindow-container .remoteControlWindow-bottom view {
  width: 50%;
  height: 80rpx;
}

.remoteControlWindow-container .remoteControlWindow-bottom button {
  width: 260rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  border-radius: 15rpx;
}

.remoteControlWindow-container .remoteControlWindow-bottom navigator {
  width: 280rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: auto;
  font-size: 30rpx;
  border-radius: 15rpx;
}

/* ================================= 热点商务联系广告面板 ================================= */
.advert-panel {
  height: 90rpx;
  margin-top: 10rpx;
  font-size: 30rpx;
}

.advert-panel>label,
.advert-panel>button {
  display: block;
}

.advert-panel>button {
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 5rpx;
  font-size: 26rpx;
}

.advert-panel>.fa {
  margin-left: 30rpx;
  margin-right: 0rpx;
}

.advert-panel>.msg {
  margin-left: 20rpx;
  margin-right: 200rpx;
}

/* ================================= 打断投屏弹窗 ================================= */
.interrupt-launch-screen-window-container {
  width: 100%;
  height: 100%;
  text-align: left;
  border-radius: 10rpx;
}

.interrupt-launch-screen-window-container>.window-top {
  padding: 50rpx 0rpx 20rpx 0rpx;
}

.interrupt-launch-screen-window-container>.window-top>.title>label {
  font-weight: bold;
  padding-left: 20rpx;
}

.interrupt-launch-screen-window-container>.window-main {
  padding: 20rpx 40rpx;
  margin-bottom: 20rpx;
}

.interrupt-launch-screen-window-container>.window-main>.content {
  display: block;
  margin-bottom: 20rpx;
}

.interrupt-launch-screen-window-container>.window-bottom {
  height: 120rpx;
}

.interrupt-launch-screen-window-container>.window-bottom>button {
  width: 285rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  display: inline-block;
  margin: 0rpx 40rpx;
}

/* ================================= 自定义顶级页面导航栏 ================================= */
.default-navigation-bar {
  width: 100vw;
}

.default-navigation-bar>.status-bar {
  font-size: 52rpx;
}

.default-navigation-bar>.navigation-bar {
  height: 46px;
}

.default-navigation-bar>.navigation-bar>.btn {
  width: 60px;
}

.default-navigation-bar>.navigation-bar>.title {
  width: calc(100% - 120px - 40rpx);
}

.default-navigation-bar>.navigation-bar>.title>.text {
  font-size: 36rpx;
  font-weight: bold;
}

/* ================================= 自定义二级页面导航栏 ================================= */
.default-navigation-bar>.navigation-bar>.btn {
  padding-left: 20rpx;
}

.default-navigation-bar>.navigation-bar>.btn>.go-back,
.default-navigation-bar>.navigation-bar>.btn>.to-home {
  padding-left: 5px;
  padding-right: 5px;
  font-size: 28rpx;
}

.default-navigation-bar>.navigation-bar>.btn>.go-back>image,
.default-navigation-bar>.navigation-bar>.btn>.to-home>image {
  width: 24px;
  height: 24px;
  vertical-align: middle;
}

.default-navigation-bar>.navigation-bar>.btn>.go-back>label,
.default-navigation-bar>.navigation-bar>.btn>.to-home>label {
  width: 24px;
  height: 24px;
}

/* ======================================= 无网页面 ======================================= */
.no-net {
  width: 500rpx;
  height: 500rpx;
}

.no-net>image {
  width: 200rpx;
  height: 200rpx;
}

.no-net>text {
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
}

.no-net>view {
  width: 100%;
  height: 60rpx;
  margin-top: 160rpx;
}

.no-net>view>.theme-button-dark,
.no-net>view>.theme-button-like {
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  border-radius: 10rpx;
}


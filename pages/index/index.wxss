.page_main_h > .tip > button {
  width: 180rpx;
  height: 50rpx;
  margin-top: 50rpx;
  padding: unset;
  line-height: 2;
  font-size: 24rpx;
  border: 2rpx solid #f6f2ec;
  background-color: unset;
}

.page_main_h > .tip > button::before, .page_main_h > .tip > button::after {
  border: 2rpx solid #f6f2ec;
}

.page_main_h > .operation {
  height: 600rpx;
}

.page_main_h > .operation > view {
  align-items: center;
  padding: 20rpx 0rpx 20rpx 0rpx;
}

.page_main_h > .operation > view > image {
  width: 48rpx;
  height: 38rpx;
  vertical-align: middle;
}

.page_main_h > .operation > view > label {
  height: 60rpx;
  line-height: 60rpx;
  margin-left: 10rpx;
}

.page_main_v {
  width: 100%;
  /* height: calc(100% - 138rpx); */
  height: 100%;
  background-color: #932c3f;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.page_main_h {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.page_main_h > .tip {
  height: 40rpx;
  padding-bottom: 200rpx;
}
.page_main_h > .climbtree{
  width: 640rpx;
  height: 286rpx;
  padding: unset;
  display: flex;
}
image.input_pic_banner {
  width: 695rpx;
  height: 200rpx;
}

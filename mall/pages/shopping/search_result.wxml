						<view class="top flex-column-flex_start-flex_start">
							<image class="pic" mode="aspectFill" src="https://oss.littlehotspot.com/WeChat/MiniProgram/LaunchScreen/source/images/imgs/default.jpeg"></image>
						</view>
						<view class="bottom flex-column-flex_start-flex_start">
							<view class="name two-line-overflow">大董 年货礼盒限量款2400g 烤鸭糕点年糕组合</view>
							<view class="bottom flex-row-center-space_between">
								<view class="left flex-row-center-flex_start">
									<view class="price theme-font5">￥<text>198</text></view>
									<view class="price market theme-font3">￥298</view>
								</view>
								<view class="right flex-row-center-flex_end">
									<view class="fa fa-television theme-font" aria-hidden="true"></view>
									<view class="fa fa-cart-plus theme-font5" aria-hidden="true"></view>
								</view>
							</view>
						</view>
					</view>
					<view class="item flex-column-center-flex_start theme-panel">

.one-line-overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.two-line-overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

button,
button::before,
button::after {
  border: unset;
  padding: unset;
  margin: auto;
}

button {
  border-radius: 10rpx;
}

button::before,
button::after {
  background-color: unset;
}

.button_none,
.button_none::before,
.button_none::after {
  margin: inherit;
  border: inherit;
  padding: inherit;
  border-radius: inherit;
  background: inherit;
  font-size: inherit;
  color: inherit;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}


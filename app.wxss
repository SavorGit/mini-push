/* ================== 表格布局开始 ================== */

.weui-grids,
.weui-grid {
  position: relative;
}

.weui-grids {
  overflow: hidden;
}

.weui-grids::before,
.weui-grids::after,
.weui-grid::before,
.weui-grid::after {
  content: " ";
  position: absolute;
  /*color: #d9d9d9; */
}

.weui-grids::before,
.weui-grids::after {
  top: 0;
  left: 0;
  transform-origin: 0 0;
  -webkit-transform-origin: 0 0;
}

.weui-grids::before,
.weui-grid::after {
  height: 1px;
  right: 0;
  transform: scaleY(0.5);
  -webkit-transform: scaleY(0.5);
}

.weui-grids::after,
.weui-grid::before {
  width: 1px;
  bottom: 0;
  -webkit-transform: scaleX(0.5);
  transform: scaleX(0.5);
}

/* .weui-grids::before {
  border-top: 1px solid #d9d9d9;
}

.weui-grids::after {
  border-left: 1px solid #d9d9d9;
} */

.weui-grid {
  float: left;
  width: 33.33333333%;
  box-sizing: border-box;
}

.weui-grid::before {
  top: 0;
  right: 0;
  /* border-right: 1px solid #d9d9d9; */
  -webkit-transform-origin: 100% 0;
  transform-origin: 100% 0;
}

.weui-grid::after {
  left: 0;
  bottom: 0;
  /* border-bottom: 1px solid #d9d9d9; */
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
}

.weui-grid:active {
  background-color: #ececec;
}

/* ================== 表格布局结束 ================== */

.guided_mask_panel {
  position: absolute;
  top: 0;
  left: 0;
  background-color: #000;
  opacity: 0.9;
}

